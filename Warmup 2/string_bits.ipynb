   "source": [
    "def string_bits(str):\n",
    "    result = \"\"\n",
    "    n = range(len(str))\n",
    "    for i in n:\n",
    "            if i % 2 == 0:\n",
    "                result = result + str[i]\n",
    "    return result"
   ]
